{
    "version": 2,
    "builds": [
      {
        "src": "cashzen/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "cashzen/dist"
        }
      },
      {
        "src": "backend/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "cashzen/dist/index.html"
      }
    ]
  }
  